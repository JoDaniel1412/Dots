<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sSettings">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <Label text="Settings" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="FullScreen" />
                        <Button mnemonicParsing="false" onAction="#pressed_fullscreen" text="On" />
                        <Button mnemonicParsing="false" onAction="#pressed_windowed" text="Off" />
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#pressed_return" text="Return" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
